<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:bottom="0dp"
        android:left="0dp"
        android:right="0dp"
        android:top="0dp">
        <shape android:shape="oval">
            <solid android:color="#E7EAEC" />
            <corners android:radius="@dimen/_20dp" />
        </shape>
    </item>

    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape android:shape="oval">
            <corners android:radius="@dimen/_20dp" />
            <stroke
                android:width="@dimen/_3dp"
                android:color="@color/white" />
            <solid android:color="@color/color_63B128" />
        </shape>
    </item>
</layer-list>

